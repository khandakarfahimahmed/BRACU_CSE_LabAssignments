import java.util.Scanner;
public class task11
{
  public static void main (String [] args)
  {
    Scanner sc = new Scanner (System.in);
    System.out.println("ENter Quantity");
    int q = sc.nextInt();
    for (int i = 1;i<=q;i++)
    {
      for (int j=1;j<q-i;j++)
      {
        System.out.print("*");
      }
    }
  }
}